//Terminal
    "terminal.background": "#161c1e",
    "terminal.ansiGreen": "#AEC199",
    "terminal.ansiBrightGreen": "#AEC199",
    "terminal.ansiBrightYellow": "#fff099",
    "terminal.ansiBrightCyan": "#80B5B3",
//Peek View Colours
    "peekViewEditor.background": "#19262a",
    "peekView.border": "#31484f",
