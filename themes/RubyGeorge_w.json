  "tokenColors": [
    {
      "settings": {
        "foreground": "#d7dde8",
        "background": "#192123"
      }
    },
    {
      "name": "Source colour",
      "scope": "source",
      "settings": {
        "foreground": "#8fbcbb"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#d88e79ff"
        // "foreground": "#d39b67"
        // "foreground": "#d9c679"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#9c9d9dff"
      }
    },
    {
      "name": "Numeric",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#609ad2ff"
      }
    },
    {
      "name": "Keyword Self",
      "scope": [
        "variable.language",
        "variable.other.readwrite.instance",
        "variable.parameter.function.language.special.self"
      ],
      "settings": {
        "foreground": "#d9c679"
      }
    },
    {
      "name": "Keyword Special",
      "scope": [
        "keyword.other.special-method",
        "meta.require",
        "entity.name.function.special",
        "support.function.core",
        "support.function",
        "keyword.control.import",
        "storage.modifier.declaration"
      ],
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "Punctuation Seperator Variable",
      "scope": "punctuation.separator.variable",
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        // "foreground": "#4C7749"
        "foreground": "#9c9d9dff"
      }
    },
    {
      "name": "Class Support",
      "scope": [
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Symbol",
      "scope": "constant.language.symbol",
      "settings": {
        "foreground": "#609ad2ff"
      }
    },
    {
      "name": "Constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#d9c679"
      }
    },
    {
      "name": "Keyword Operator Arithmetic",
      "scope": "keyword.operator.arithmetic",
      "settings": {
        "foreground": "#9c9d9dff"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "entity.name.function",
        "meta.function-call.generic"
      ],
      "settings": {
        // "foreground": "#5cb1bc"
        "foreground": "#609ad2ff"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Keyword Operator Comparison",
      "scope": "keyword.operator.comparison",
      "settings": {
        // "foreground": "#d39b67"
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Punctuation Embedded",
      "scope": [
        "punctuation.section.embedded",
        "constant.character.format.placeholder"
      ],
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "Property Name",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "True/ False",
      "scope": "constant.language",
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5a5f68"
      }
    },
    {
      "name": "Keyword Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "Punctuation String",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Punctuation Preprocessor",
      "scope": "meta.preprocessor punctuation",
      "settings": {
        "foreground": "#6b6459"
      }
    },
    {
      "name": "String Preprocessor",
      "scope": "meta.preprocessor string",
      "settings": {
        "foreground": "#7d705d"
      }
    },
    {
      "name": "Storage Type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Type Name",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#d88e79"
      }
    },
    {
      "name": "Illegal",
      "scope": "illegal",
      "settings" : {
        "foreground": "#aa250a"
      }
    }
  ]
}
