  },
//Scopes
  "tokenColors": [
    {
      "settings": {
        "foreground": "#d7dde8",
        "background": "#192123"
      }
    },
    {
      "name": "Source colour",
      "scope": [
        "source",
        "variable"
      ],
      "settings": {
        // "foreground": "#8fbcbb"
        "foreground": "#749E94" //better??
      }
    },
    {
      "name": "Variable member",
      "scope": [
        "variable.other.member"
      ],
      "settings": {
        "foreground": "#d9c679"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control",
        "storage.type.class",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#d88e79ff"
        // "foreground": "#d39b67"
        // "foreground": "#d9c679"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.function.method.with-arguments",
        "meta.brace",
        "keyword.operator.type"
      ],
      "settings": {
        "foreground": "#9c9d9dff"
      }
    },
    {
      "name": "Numeric",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#609ad2ff"
      }
    },
    {
      "name": "Keyword Self",
      "scope": [
        "variable.language",
        "variable.other.readwrite.instance",
        "variable.parameter.function.language.special.self"
      ],
      "settings": {
        "foreground": "#d9c679"
      }
    },
    {
      "name": "Keyword Special",
      "scope": [
        "keyword.other.special-method",
        "entity.name.function.special",
        "meta.method.declaration",
        "meta.require",
        "keyword.control.import",
        "keyword.control.require",
        "support.function.core",
        "support.function",
        "storage.modifier.declaration",
        "storage.modifier",
        "storage.type"
      ],
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "Punctuation Separator Variable",
      "scope": [
        "punctuation.separator.variable",
        "punctuation.separator.arguments"
      ],
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        // "foreground": "#4C7749"
        "foreground": "#9c9d9dff"
      }
    },
    {
      "name": "Class Support",
      "scope": [
        "support.class",
        "support.type",
        "entity.other.inherited-class",
        "support.variable"
      ],
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Symbol",
      "scope": [
        "constant.language.symbol",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#609ad2ff"
      }
    },
    {
      "name": "Constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#d9c679"
      }
    },
    {
      "name": "Keyword Operator Arithmetic",
      "scope": "keyword.operator.arithmetic",
      "settings": {
        "foreground": "#9c9d9dff"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "entity.name.function",
        "meta.function-call.generic"
      ],
      "settings": {
        // "foreground": "#5cb1bc"
        "foreground": "#609ad2ff"
      }
    },
    {
      "name": "Function class name",
      "scope": [
        "entity.name.function.class"
      ],
      "settings": {
        "foreground": "#d9c679"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Keyword Operator Comparison",
      "scope": "keyword.operator.comparison",
      "settings": {
        // "foreground": "#d39b67"
        "foreground": "#cea47f"
      }
    },
    {
      "name": "Punctuation Embedded",
      "scope": [
        "punctuation.section.embedded",
        "constant.character.format.placeholder",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "Property Name",
      "scope": [
				"support.type.property-name",
				"entity.name.tag"
			],
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "True/ False",
      "scope": "constant.language",
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5a5f68"
      }
    },
    {
      "name": "Keyword Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#98816eff"
      }
    },
    {
      "name": "Punctuation String",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Punctuation Preprocessor",
      "scope": "meta.preprocessor punctuation",
      "settings": {
        "foreground": "#6b6459"
      }
    },
    {
      "name": "String Preprocessor",
      "scope": "meta.preprocessor string",
      "settings": {
        "foreground": "#7d705d"
      }
    },
    {
      "name": "Storage Type",
      "scope": [
        "storage.type.c",
        "storage.type.cpp"
      ],
      "settings": {
        "foreground": "#b6776b"
      }
    },
    {
      "name": "Type Name",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#d88e79"
      }
    }
  ]
}
